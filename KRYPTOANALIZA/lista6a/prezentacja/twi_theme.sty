% twi_theme.sty
\ProvidesPackage{mytheme}

\usetheme{default}
\useoutertheme{shadow}
\setbeamertemplate{navigation symbols}{}
\setbeamertemplate{blocks}[rounded]

\definecolor{maincolor}{RGB}{180,30,30}%{46,127,77}

\setbeamercolor{titlelike}{fg=white,bg=maincolor}
\setbeamercolor{frametitle}{fg=white,bg=maincolor}
\setbeamercolor{block title}{fg=white,bg=maincolor}

\setbeamertemplate{footline}[frame number]

